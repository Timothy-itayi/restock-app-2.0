{
  "cli": {
    "version": ">= 13.0.0",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      }
    },
    "preview": {
      "distribution": "store",
      "ios": {
        "simulator": false
      },
      "channel": "preview"
    },
    "production": {
      "distribution": "store",
      "ios": {
        "autoIncrement": "buildNumber"
      },
      "android": {
        "autoIncrement": "versionCode"
      },
      "channel": "production"
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "timothy_itayi@icloud.com",
        "ascAppId": "6755952370",
        "appleTeamId": "WRJXN2DGS3"
      }
    }
  }
}

