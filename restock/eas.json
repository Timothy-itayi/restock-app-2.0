{
  "cli": {
    "version": ">= 13.0.0",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "env": {
        "COMPANY_API_URL": "https://restock-company.parse-doc.workers.dev",
        "PARSE_DOC_API_URL": "https://restock-parse-doc.parse-doc.workers.dev",
        "SEND_EMAIL_API_URL": "https://restock-send-email.parse-doc.workers.dev"
      }
    },
    "preview": {
      "distribution": "store",
      "ios": {
        "simulator": false
      },
      "channel": "preview",
      "env": {
        "COMPANY_API_URL": "https://restock-company.parse-doc.workers.dev",
        "PARSE_DOC_API_URL": "https://restock-parse-doc.parse-doc.workers.dev",
        "SEND_EMAIL_API_URL": "https://restock-send-email.parse-doc.workers.dev"
      }
    },
    "production": {
      "distribution": "store",
      "ios": {
        "autoIncrement": "buildNumber"
      },
      "android": {
        "autoIncrement": "versionCode"
      },
      "channel": "production",
      "env": {
        "COMPANY_API_URL": "https://restock-company.parse-doc.workers.dev",
        "PARSE_DOC_API_URL": "https://restock-parse-doc.parse-doc.workers.dev",
        "SEND_EMAIL_API_URL": "https://restock-send-email.parse-doc.workers.dev"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "timothy_itayi@icloud.com",
        "ascAppId": "6755952370",
        "appleTeamId": "WRJXN2DGS3"
      }
    }
  }
}

