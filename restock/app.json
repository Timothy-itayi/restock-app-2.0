{
  "expo": {
    "name": "Restock",
    "slug": "restock",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/restock-icon-adjusted.png",
    "scheme": "restock",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/restock-icon-adjusted.png",
      "resizeMode": "contain",
      "backgroundColor": "#1A1A1A"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.timothy-itayi.restock",
      "buildNumber": "10",
      "infoPlist": {
        "NSCameraUsageDescription": "Restock needs camera access to photograph inventory catalogs for parsing.",
        "NSPhotoLibraryUsageDescription": "Restock needs photo library access to select catalog images for parsing.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.timothyitayi.restock",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/restock-icon-adjusted.png",
        "backgroundColor": "#1A1A1A"
      }
    },
    "plugins": [
      "expo-router",
      "expo-font",
      [
        "@sentry/react-native/expo",
        {
          "url": "https://sentry.io/",
          "project": "react-native",
          "organization": "timothy-itayi"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "855519b7-bda3-4211-ba09-dea4d811f30a"
      },
      "COMPANY_API_URL": "https://restock-company.parse-doc.workers.dev",
      "PARSE_DOC_API_URL": "https://restock-parse-doc.parse-doc.workers.dev",
      "SEND_EMAIL_API_URL": "https://restock-send-email.parse-doc.workers.dev"
    },
    "owner": "timothy-itayi",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/855519b7-bda3-4211-ba09-dea4d811f30a"
    }
  }
}
